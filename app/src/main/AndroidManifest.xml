<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kriti.myfirstapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyFirstApp">
        <activity android:name=".MusicPlayer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyBottomNavActivity">

        </activity>
        <activity android:name=".Gallery"></activity>
        <activity android:name=".MyListView"></activity>
        <activity android:name=".SplashScreen" />
        <activity android:name=".DisplayPref" />
        <activity android:name=".MyPreferences" />

        <receiver android:name=".MyReceiver" />

        <activity android:name=".AlarmActivity" />
        <activity android:name=".BufferFile" />
        <activity android:name=".MyFile" />
        <activity android:name=".Currency" />
        <activity android:name=".LinearCalculator" />
        <activity android:name=".SimpleCalculator" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>